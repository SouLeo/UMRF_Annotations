<launch>
  <arg name="umrf_file" default="$(find data_transfer)/test/invoker_umrf.json"/>
  <arg name="wake_word" default="data_transfer"/>
  <arg name="action_path" default="$(find data_transfer)"/>

  <node name="temoto_action_engine" pkg="temoto_action_engine" type="action_engine_node" output="screen"
        args="--sa $(arg action_path) 
              --d $(arg umrf_file) 
              --mw $(arg wake_word)"/>
</launch>